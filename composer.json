{
  "name": "cornfield-io/cornfield",
  "description": "",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Cornfield\\": "src/"
    }
  },
  "scripts": {
    "dev": [
      "@require-check",
      "@fix-style",
      "@quality"
    ],
    "fix-style": [
      "php-cs-fixer fix src --rules=@Symfony"
    ],
    "quality": [
      "psalm",
      "phpstan analyse --level max src"
    ],
    "require-check": [
      "composer-require-checker check ./composer.json"
    ]
  },
  "require": {
    "php": ">=7.2",
    "cornfield-io/core": "dev-development"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15",
    "maglnet/composer-require-checker": "^2.0",
    "phpstan/phpstan": "^0.11.16",
    "phpstan/phpstan-strict-rules": "^0.11.1",
    "roave/no-floaters": "^1.1",
    "roave/security-advisories": "dev-master",
    "vimeo/psalm": "^3.5"
  },
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/cornfield-io/core",
      "no-api": true
    }
  ]
}
